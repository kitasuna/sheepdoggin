-- globals for controlling time stuff
_now = 0
_last_ts = 0

-- main
function _init()
  printh("--init")

  _now = time()
  _last_ts = _now

  sheep_mgr = SheepMgr:new()
  sheep_mgr:spawn()
  physics = Physics:new()

  __update = update_game
  __draw = draw_game
end

function _update60()
  _now = time()
  local dt = _now - _last_ts
  __update(dt)
  _last_ts = _now
end

function _draw()
  __draw()
end

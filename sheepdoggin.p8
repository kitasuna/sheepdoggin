#include _game.lua
#include _title.lua
#include player.lua
#include sheep.lua
#include vector2.lua

-- globals for controlling time stuff
_now = 0
_last_ts = 0

-- main
function _init()
  printh("--init")

  _now = time()
  _last_ts = _now

  SheepMgr:init()

  __update = update_game
  __draw = draw_game
end

function _update60()
  _now = time()
  local dt = _now - _last_ts
  __update(dt)
  _last_ts = _now
end

function _draw()
  __draw()
end

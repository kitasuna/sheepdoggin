#include sheep.lua
#include vector2.lua
#include camerafollow.lua

-- globals for controlling time stuff
_now = 0
_last_ts = 0

-- main
function _init()
  printh("--init")

  _now = time()
  _last_ts = _now

  __update = update_game
  __draw = draw_game
  _camera_init()
end

function _update60()
  _now = time()
  local dt = _now - _last_ts
  __update(dt)
  _last_ts = _now
  _camera_update()
end

function _draw()
  __draw()
  _camera_draw()
end
